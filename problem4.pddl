(define (problem gripper-problem)
    (:domain gripper-continuous)

    (:objects
        mover1 mover2 - mover
        crate1 crate2 crate3 crate4 crate5 crate6 - crate
        loader1 loader2 - loader
        A B - group
    )

    (:init
        (mover mover1)
        (mover mover2)
        (toPositive mover1)
        (toPositive mover2)
        (free mover1)
        (free mover2)
        (equal mover1 mover1)
        (equal mover2 mover2)
        (= (rob_position mover1) 0)  
        (= (rob_position mover2) 0)  
        (not (moving mover1))
        (not (moving mover2))
        (= (battery mover1) 20)
        (= (battery mover2) 20)
        (= (maxBattery) 20)
        
        (= (max_vel mover1) 10)
        (= (max_vel mover2) 10)
        
        (= (velocity mover1) 10)
        (= (velocity mover2) 10)

        (not(coefficientSet))
        
        (crate crate1)
        (crate crate2)
        (crate crate3)
        (crate crate4)
        (crate crate5)
        (crate crate6)
        (not (isLoaded crate1))
        (not (isLoaded crate2))
        (not (isLoaded crate3))
        (not (isLoaded crate4))
        (not (isLoaded crate5))
        (not (isLoaded crate6))
        (not (isCarried crate1))
        (not (isCarried crate2))
        (not (isCarried crate3))
        (not (isCarried crate4))
        (not (isCarried crate5))
        (not (isCarried crate6))
        (= (position crate1) 20)
        (= (position crate2) 20)
        (= (position crate3) 10)
        (= (position crate4) 20)
        (= (position crate5) 30)
        (= (position crate6) 10)
        (= (weight crate1) 30)
        (= (weight crate2) 20)
        (= (weight crate3) 30)
        (= (weight crate4) 20)
        (= (weight crate5) 30)
        (= (weight crate6) 10)
        (not (isFragile crate1))
        (isFragile crate2)
        (isFragile crate3)
        (isFragile crate4)
        (isFragile crate5)
        (not (isFragile crate6))
        (= (belong crate1) 1)
        (= (belong crate2) 1)
        (= (belong crate3) 2)
        (= (belong crate4) 2)
        (= (belong crate5) 2)
        (= (belong crate6) 0)
        (group A)
        (group B)
        (= (numOfGroup A) 1)
        (= (numOfGroup B) 2)
        (= (elementsPerGroup A) 2)
        (= (elementsPerGroup B) 3)
        
        (not (currentGroupSet))
        (= (currentGroup) 0)

        (loader loader1)
        (loader loader2)
        (= (loaderTimer loader1) 0)
        (= (loaderTimer loader2) 0)
        (freeLoader loader1)
        (freeLoader loader2) 
        (not (isCheap loader2))
        (isCheap loader1)
    )

    (:goal
        (and 
            (isLoaded crate1)
            (isLoaded crate2)
            (isLoaded crate3)
            (isLoaded crate4)
            (isLoaded crate5)
            (isLoaded crate6)
        )
    )

    ; (:metric minimize (total-time))

)
